@model IEnumerable<Jupiter.Components.DBModels.common_Exceptions>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table>
    <tr>
        <th>
            Message
        </th>
        <th>
            Source
        </th>
        <th>
            UserHostAddress
        </th>
        <th>
            CreateDate
        </th>
        <th>
            State
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td title="@Html.DisplayFor(modelItem => item.StackTrace)">
            @Html.ActionLink(item.Message, "Details", new { id = item.Id })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Source)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserHostAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.common_Exceptions_State.Name)
        </td>
    </tr>
}

</table>
</div>